{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["ie >= 11"]
        },
        "exclude": ["transform-async-to-generator", "transform-regenerator"],
        "modules": false,
        "loose": true
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": ["transform-class-properties", "transform-object-rest-spread"],
  "env": {
    "commonjs": {
      "presets": [
        [
          "env",
          {
            "loose": true
          }
        ],
        "stage-0",
        "react"
      ]
    },
    "test": {
      "presets": ["env"],
      "plugins": ["transform-class-properties", "transform-object-rest-spread"]
    }
  }
}
